{
    "name": "pip_init_basic",
    "args": [
        {
            "name": "project_name",
            "description": "The name of project"
        },
        {
            "name": "dependencies",
            "description": "Dependencies of project",
            "argtype": "array"
        },
        {
            "name": "mypy_python_version",
            "description": "Version of Python3 interpreter (for mypy)",
            "default_value": "3.9"
        }
    ],
    "args_handler": "src.arg_handler",
    "contents": [
        {
            "src": "setup.cfg",
            "dest": "./setup.cfg"
        },
        {
            "src": "setup.py",
            "dest": "./setup.py"
        },
        {
            "src": "__init__.py",
            "dest": "./{project_name}/__init__.py"
        }
    ]
}